<UserControl x:Class="WinUIDesignAndAnimationLab.Demos.GalaxyShuttles.GalaxyShuttleDemo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:galaxyshuttles="using:WinUIDesignAndAnimationLab.Demos.GalaxyShuttles"
             d:DesignHeight="300"
             d:DesignWidth="600"
             mc:Ignorable="d">

    <Grid x:Name="Root">
        <!--<Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>-->
        <galaxyshuttles:GalaxyShuttleControl Delay="0:0:4" RenderTransformOrigin="0.5,0.5">
            <galaxyshuttles:GalaxyShuttleControl.RenderTransform>
                <CompositeTransform Rotation="25" />
            </galaxyshuttles:GalaxyShuttleControl.RenderTransform>
        </galaxyshuttles:GalaxyShuttleControl>
        <galaxyshuttles:GalaxyShuttleControl Grid.Column="1" />
    </Grid>
</UserControl>